    "secondary": {
      "border": "",
      "radius": ""
    }
  },
  "palette": {
    "modal": {
      "backdrop": {
        "primary": "0, 0, 0, 0.5",
        "secondary": "0, 0, 0, 0.9"
      },
      "background": {
        "primary": "",
        "secondary": ""
      }
    }
  },
  "footer": {
