    "buttonPrimary": {
      "border": "0",
      "background": "#30a74b",
      "color": "#ffffff",
      "radius": "0.25rem"
    },
    "buttonSecondary": {
      "border": ".0625rem solid #000000",
      "background": "#ffffff",
      "color": "#000000",
      "radius": "0.25rem"
    },
    "buttonTertiary": {
      "border": "0",
      "background": "#000000",
      "color": "#ffffff",
      "radius": "0.25rem"
    },
    "input": {
      "radius": "0.25rem",
      "color": "#000",
      "background": "#fff",
      "border": "0.0625rem solid #eaeaea",
      "placeholder": "",
      "focusActive": {
        "background": ""
