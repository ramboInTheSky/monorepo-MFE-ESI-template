    "disabled": {
      "border": ".0625rem solid #dedede",
      "background": "#ffffff",
      "radius": "0.25rem"
    },
    "focusActive": {
      "border": ".0625rem solid #000000",
      "background": "#ffffff",
      "radius": "0.25rem"
    },
    "invalid": {
      "border": ".0625rem solid #d91440",
      "background": "#ffffff",
      "radius": "0.25rem"
    },
    "selectedAccent": {
      "border": ".125rem solid #257F39",
      "background": "#ffffff",
      "radius": "0.25rem"
