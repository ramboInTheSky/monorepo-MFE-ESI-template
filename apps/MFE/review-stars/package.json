{
  "name": "ecommerce-reviewstars-frontend",
  "version": "3.3.2",
  "private": true,
  "scripts": {
    "build": "cross-env LOG_LEVEL=warn NODE_ENV=production node scripts/build.js",
    "build:local": "cross-env LOG_LEVEL=warn GENERATE_SOURCEMAP=false DEV_URL_OVERRIDE=http://localhost:3002 ASSETS_PATH=/reviewstarsstatic NODE_ENV=production node scripts/build.js",
    "cypress": "cypress open --browser chrome",
    "dev": "cross-env NODE_ICU_DATA='node_modules/full-icu' node scripts/start.js",
    "dev:headless": "cross-env REACT_APP_API_BASEURL=http://localhost:3037 LOG_LEVEL=warn npm-run-all -p stub-review-stars-api  dev",
    "lint": "eslint --ext .tsx,.ts src/",
    "stub-review-stars-api": "node dev_review_stars_api/index.js",
    "test": "cross-env NODE_ICU_DATA='node_modules/full-icu' jest src/",
    "test-ci": "cross-env NODE_ICU_DATA='node_modules/full-icu' jest src/ --ci --reporters=default --reporters=jest-junit",
    "test:integration": "cross-env DEV_URL_OVERRIDE=http://localhost:3333 REACT_APP_API_BASEURL=http://localhost:3037 NODE_ENV=production START_SERVER_AND_TEST_INSECURE=1 npm-run-all -p -r stub-review-stars-api cypress:integration",
    "cypress:integration": "start-server-and-test start:container http://localhost:3007/review-stars/1234  \"cypress run --config video=false \"",
    "test:integration:interactive": "cross-env DEV_URL_OVERRIDE=http://localhost:3333 REACT_APP_API_BASEURL=http://localhost:3037 NODE_ENV=production START_SERVER_AND_TEST_INSECURE=1 npm-run-all -p stub-review-stars-api cypress:interactive",
    "cypress:interactive": "start-server-and-test start:container http://localhost:3007/review-stars/1234 \"cypress open --browser chrome \"",
    "tsc": "tsc",
    "start": "cross-env NODE_ICU_DATA='node_modules/full-icu' NODE_ENV=production node ./build/index.js",
    "start:container": "cross-env npm-run-all -p -r container start",
    "start:headless": "cross-env DEV_URL_OVERRIDE=http://localhost:3007 NODE_ENV=production REACT_APP_API_BASEURL=http://localhost:3037 npm-run-all -p stub-review-stars-api start",
    "start:headless:container": "cross-env DEV_URL_OVERRIDE=http://localhost:3007 NODE_ENV=production REACT_APP_API_BASEURL=http://localhost:3037 npm-run-all -p stub-review-stars-api start:container",
    "container": "node review-stars-container-service/index.js",
    "precommit": "npm run tsc && npm run test -- --coverage && jest-coverage-ratchet && git add package.json && npm run build:local && npm run test:integration"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/*.cy.{ts,tsx}"
    ],
    "coverageReporters": [
      "json-summary",
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 95.09,
        "statements": 95.35,
        "functions": 96.55,
        "branches": 76.19
      }
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "@mockData/(.*)": "<rootDir>/__mocks__/$1",
      "^@app/(.*)$": "<rootDir>/src/$1",
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "modulePaths": [],
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "./setupTests.ts",
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "./setupTestsAfterEnv.ts"
    ],
    "testEnvironment": "jest-environment-jsdom-sixteen",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "resolutions": {
    "@babel/preset-env": "^7.8.7"
  },
  "dependencies": {
    "@babel/compat-data": "^7.8.0",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.7",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@mui/material": "^5.2.8",
    "@mui/styles": "^5.2.3",
    "@monorepo/apiconfig": "^3.3.0",
    "@monorepo/babelconfig": "^3.3.0",
    "@monorepo/core-logger": "^7.3.0",
    "@monorepo/cypress-fixtures": "^5.3.0",
    "@monorepo/eslint": "^3.3.0",
    "@monorepo/eventservice": "^6.4.1",
    "@monorepo/middlewares": "^8.0.0",
    "@monorepo/settings-sdk": "^4.3.0",
    "@monorepo/star-rating": "^3.3.0",
    "@monorepo/theme-provider": "^9.0.0",
    "@monorepo/themes": "^17.0.0",
    "@monorepo/utils": "^6.4.0",
    "@svgr/webpack": "4.3.2",
    "@testing-library/react": "^9.5.0",
    "@types/express": "4.17.4",
    "@types/helmet": "0.0.45",
    "@types/jest": "25.1.5",
    "@types/morgan": "^1.9.0",
    "@types/node": "13.11.0",
